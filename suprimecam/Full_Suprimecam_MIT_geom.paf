    pixelSize: 1.0 # All positions in pixels, rather than mm
    offsetUnit: "pixels"
    ptype: "bias-left"
    nCol: 1
    nRow: 1 
    Amp: { 
        serial: 1
        ptype: "bias-left"
        flipLR: false
        nQuarter: 0
        hdu: 0
        diskCoordSys: "camera"
